{
    "name": "esl-ionic",
    "version": "0.0.1",
    "description": "An Ionic project",
    "license": "Apache-2.0",
    "config": {
        "ionic_webpack": "./src/config/webpack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ionic-team/ionic-conference-app.git"
    },
    "scripts": {
        "build": "ionic-app-scripts build",
        "clean": "ionic-app-scripts clean",
        "lint": "ionic-app-scripts lint",
        "ionic:build": "npm run build",
        "ionic:serve": "npm run serve",
        "ionic:lint": "npm run lint",
        "test": "karma start ./test-config/karma.conf.js",
        "test-ci": "karma start ./test-config/karma.conf.js --single-run",
        "test-coverage": "karma start ./test-config/karma.conf.js --coverage",
        "e2e": "npm run e2e-update && npm run e2e-test",
        "e2e-test": "protractor ./test-config/protractor.conf.js",
        "e2e-update": "webdriver-manager update --standalone false --gecko false"
    },
    "dependencies": {
        "@angular/animations": "^5.2.6",
        "@angular/common": "5.0.0",
        "@angular/compiler": "5.0.0",
        "@angular/compiler-cliA": "5.0.0",
        "@angular/core": "5.0.0",
        "@angular/forms": "5.0.0",
        "@angular/http": "5.0.0",
        "@angular/platform-browser": "5.0.0",
        "@angular/platform-browser-dynamic": "5.0.0",
        "@auth0/cordova": "^0.3.0",
        "@ionic-native/core": "^4.7.0",
        "@ionic-native/google-analytics": "^4.7.0",
        "@ionic-native/in-app-browser": "^4.7.0",
        "@ionic-native/splash-screen": "^4.7.0",
        "@ionic-native/status-bar": "^4.7.0",
        "@ionic-native/text-to-speech": "^4.7.0",
        "@ionic/storage": "2.1.3",
        "@ngx-translate/core": "^9.1.1",
        "@ngx-translate/http-loader": "^2.0.1",
        "auth0-js": "^9.3.0",
        "cordova-android": "6.3.0",
        "cordova-android-support-gradle-release": "^1.4.2",
        "cordova-browser": "5.0.3",
        "cordova-ios": "4.5.4",
        "cordova-plugin-crosswalk-webview": "^2.4.0",
        "cordova-plugin-customurlscheme": "^4.3.0",
        "cordova-plugin-device": "^1.1.7",
        "cordova-plugin-google-analytics": "^1.8.3",
        "cordova-plugin-inappbrowser": "^1.7.2",
        "cordova-plugin-ionic-keyboard": "^2.0.5",
        "cordova-plugin-ionic-webview": "^1.1.16",
        "cordova-plugin-safariviewcontroller": "^1.5.2",
        "cordova-plugin-splashscreen": "^4.1.0",
        "cordova-plugin-statusbar": "^2.4.1",
        "cordova-plugin-tts": "^0.2.3",
        "cordova-plugin-whitelist": "^1.3.3",
        "emulate": "1.0.2",
        "ionic-angular": "3.9.2",
        "ionicons": "3.0.0",
        "rxjs": "5.5.2",
        "sw-toolbox": "3.6.0",
        "web-animations-js": "^2.3.1",
        "zone.js": "0.8.21"
    },
    "devDependencies": {
        "@angular/language-service": "^4.0.0",
        "@ionic/app-scripts": "3.1.9",
        "@types/jasmine": "^2.8.7",
        "@types/node": "^10.0.4",
        "angular2-template-loader": "^0.6.2",
        "html-loader": "^0.5.5",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jasmine": "^3.1.0",
        "jasmine-spec-reporter": "^4.2.1",
        "karma": "^2.0.2",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^1.4.2",
        "karma-jasmine": "^1.1.2",
        "karma-jasmine-html-reporter": "^1.0.0",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^3.0.0",
        "null-loader": "^0.1.1",
        "protractor": "^5.3.1",
        "ts-loader": "^3.5.0",
        "ts-node": "^6.0.2",
        "typescript": "~2.6.2"
    },
    "cordova": {
        "platforms": [
            "browser",
            "android",
            "ios"
        ],
        "plugins": {
            "cordova-plugin-crosswalk-webview": {
                "XWALK_VERSION": "22+",
                "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
                "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
                "XWALK_MODE": "embedded",
                "XWALK_MULTIPLEAPK": "true"
            },
            "cordova-plugin-device": {},
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-ionic-webview": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-ionic-keyboard": {},
            "cordova-plugin-safariviewcontroller": {},
            "cordova-plugin-customurlscheme": {
                "URL_SCHEME": "com.esl.ionic",
                "ANDROID_SCHEME": "com.esl.ionic",
                "ANDROID_HOST": "thcathy.auth0.com",
                "ANDROID_PATHPREFIX": "/cordova/com.esl.ionic/callback"
            },
            "cordova-plugin-google-analytics": {},
            "cordova-plugin-tts": {}
        }
    }
}